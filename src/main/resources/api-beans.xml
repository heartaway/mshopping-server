<?xml version="1.0" encoding="GBK"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
       default-autowire="byName">

    <bean id="appConfig" class="com.alibaba.demo.model.AppConfig">
        <property name="appKey" value="${appKey}"/>
        <property name="appSecret" value="${appSecret}"/>
        <property name="envValue" value="${env}"/>
        <property name="local" value="${isLocal}"/>
        <property name="owner" value="${owner}"/>
        <property name="oauth" value="${isOauth}"/>
        <property name="contextName" value="${contextName}"/>

        <property name="dataSourceAlias" value="${dataSourceAlias}"/>
    </bean>


    <context:component-scan base-package="com.alibaba.demo.api"/>

    <import resource="task.xml"/>
</beans>
